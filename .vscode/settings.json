{
  "files.associations": {
    "**/package.json": "json",
    "**/*.json": "jsonc"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/.history": true,
    "**/dist": true
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.tslint": true
  }
}
